{:source-paths ["src"]
 :dependencies []
 :nrepl {:port 9000}
 :deps {:aliases [:dev]}
 :builds {:app {:target :browser
                :output-dir "resources/public/js"
                :asset-path "/js"
                :modules {:main {:init-fn client.core/init}}
                :devtools {:http-root "resources/public"
                           :http-port 8020}
                :build-hooks [(dev/extract-classes)]
                :watch-dirs ["src" "resources/public"]}}}